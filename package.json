    "autoprefixer": "^10.2.5",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "eslint-plugin-unused-imports": "^1.1.0",
    "nollup": "^0.15.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.0.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-svelte-hot": "^1.0.0-6",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.35.0",
    "svelte-hero-icons": "^1.6.0-next.4",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.3"
  },
  "scripts": {
    "build": "routify -b && rollup -c",
    "autobuild": "rollup -c -w --environment LIVERELOAD",
    "routify": "routify",
    "dev": "run-p routify dev:nollup",
    "dev:nollup": "nollup --verbose -c --hot --port 5000 --content-base ./public --public-path build",
    "dev:start": "sirv public -s --dev",
    "dev:livereload": "run-p dev:start routify autobuild",
    "start": "sirv public -s"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
