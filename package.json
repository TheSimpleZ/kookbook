    "javascript-time-ago": "^2.3.4",
    "lodash.uniqby": "^4.7.0",
    "rollup-plugin-hot": "^0.1.1",
    "sveltefire": "^0.1.5",
    "tippy.js": "^6.3.1",
    "validate.js": "^0.13.0"
